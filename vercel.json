{
  "functions": {
    "api/mcp.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/mcp",
      "destination": "/api/mcp"
    },
    {
      "source": "/(.*)",
      "destination": "/api/mcp"
    }
  ],
  "build": {
    "env": {
      "PYTHONPATH": "./src"
    }
  }
}